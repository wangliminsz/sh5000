<?xml version="1.0" ?>
<odoo>

  <record id="module_demo_sh5000_show" model="ir.module.category">
    <field name="name">Demo SH5000 SHOW category</field>
  </record>

  <record id="demo_sh5000_show_group_user" model="res.groups">
    <field name="name">User</field>

    <field name="category_id" ref="module_demo_sh5000_show"/>

    <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
  </record>

  <record id="demo_sh5000_show_group_manager" model="res.groups">
    <field name="name">Manager</field>

    <field name="category_id" ref="module_demo_sh5000_show"/>

    <field name="implied_ids" eval="[(4, ref('demo_sh5000_show_group_user'))]"/>

    <field name="users" eval="[(4, ref('base.user_root')),(4, ref('base.user_admin'))]"/>
  </record>

</odoo>

